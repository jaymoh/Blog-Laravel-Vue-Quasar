{
  "@quasar/dotenv": {
    "env_development": ".env",
    "env_production": ".env.production",
    "common_root_object": "none",
    "create_env_files": false,
    "add_env_to_gitignore": true
  },
  "@quasar/testing": {
    "harnesses": [
      "unit-jest@alpha",
      "e2e-cypress@beta"
    ]
  },
  "@quasar/testing-unit-jest": {
    "babel": "babelrc",
    "options": [
      "scripts",
      "SFC",
      "wallabyjs",
      "majestic"
    ]
  }
}